<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite thread-count="3" verbose="1" name="Automation_Framework_Saucedemo"
       annotations="JDK" parallel="tests">

    <test name="First Test Suite">
        <classes>
            <class name="test.SuccesfullyTests"/>
        </classes>
    </test>
    <test name="WrongTests">
        <classes>
            <class name="test.WrongTests"/>
        </classes>
    </test>
    <test name="Wait Test">
        <classes>
            <class name="test.ProductTests"/>
        </classes>
    </test>
</suite>